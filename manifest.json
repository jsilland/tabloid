{
  "manifest_version": 2,
  "name": "Tabloid",
  "description": "Tab manager",
  "version": "0.9",
  "author": "Julien Silland",
  "homepage_url": "http://soliton.io/tabloid/",
  "minimum_chrome_version": "17",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "tabloid.html"
  },
  "permissions": [
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "mac": "Command+Shift+K",
        "windows": "Ctrl+Shift+K",
        "linux": "Ctrl+Shift+K",
        "chromeos": "Ctrl+Shift+K"
      }
    }
  }
}
